
<%#= session.inspect %>
 <% time = Time.new %>
<% @today = time.strftime("%B %d, %Y") %>
<div style="padding-top:60px;">
  <% if @today >= 1.week.ago %>
  <%= @today  %>
  <% end %>
  <%#= 39.weeks.since(Time.gm(Time.now.year)) %>
  <ul class="nav nav-pills nav-stacked">
  <li class="active">
 
      <span class="badge pull-right"><%= @jb_prods.count+@jbp_mains.count %></span>
      Total Projects
    
  </li>

  
 </ul>


<div class="project">
 <h5>JB PROD <span class="badge"><%= @jb_prods.count %></span></h5>	
<table class="table table-hover">
  <thead>
  	<tr>
	   <th>SL</th>
	   <th>PROJECT NAME</th>
	   <th>LAST UPDATE</th>
   </tr>
  </thead>
  <tbody>
  	<% @count = 1 %>
  	<% @jb_prods.each do |name| %>
  	<% @data = name.split ("@") %>
    <% date = Date.parse(@data[2]).strftime("%B %d, %Y") %>
  	 <tr>
      <% if date > 1.week.ago && date != @today %>
        <td><%= @count %></td><td><%= @data[1] %><span class="label label-default">Last <%= Date.parse(@data[2]).strftime("%A") %></span></td><td><%= date  %></td>
        <% elsif date == @today %> 
        <td><%= @count %></td><td><%= @data[1] %><span class="label label-info">Just Updated </span></td><td><%= date  %></td> 
        <% else %> 
        <td><%= @count %></td><td><%= @data[1] %></td><td><%= date
          %></td>
       <% end %> 
     </tr>
    <% @count+=1 %>
    <% end %>
  </tbody>
</table>
</div>

